{
  "compilerOptions": {
    "baseUrl": "src/app",
    "paths": {
      "@/*": ["./src/*"],
      "@/components/*": ["components/*"],
      "@/images/*": ["images/*"]
    }
  },
  "images": {
    "localPatterns": [
      {
        "pathname": "src/app/images/**",
        "search": ""
      }
    ]
  }
}
